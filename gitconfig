[color]
	ui = true

[alias]
	graph = log --graph --pretty=format:'%Cred%h%Creset - %C(cyan)<%an>%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)' --abbrev-commit --date=relative
	push = "git push; git push --tags"

[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
