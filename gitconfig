	ui = true
[alias]
	graph = log --graph --pretty=format:'%Cred%h%Creset - %C(cyan)<%an>%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)' --abbrev-commit --date=relative
	who = blame -c
[rerere]
	enabled = true
[fetch]
	prune = true
[push]
	default = current
	followTags = true
[pull]
	rebase = false
[gitflow "branch"]
	master = master
	develop = develop
[gitflow "prefix"]
	feature = feature/
	bugfix = bugfix/
	release = release/
	hotfix = hotfix/
	support = support/
	versiontag =
